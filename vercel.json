{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*",
          ".vuepress/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/"
    }
  ]
}